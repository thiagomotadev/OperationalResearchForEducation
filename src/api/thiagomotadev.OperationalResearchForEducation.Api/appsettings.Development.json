{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CorsPolicy": {
    "AllowAnyHeader": true,
    "AllowAnyMethod": true,
    "AllowAnyOrigin": true
  },
  "SwaggerUIOptions": {
    "RoutePrefix": "swagger",
    "ConfigObject": {
      "Urls": [
        {
          "Url": "/api/v1/docs/openapi.json",
          "Name": "OperationalResearchForEducation API v1"
        }
      ],
      "DeepLinking": true,
      "DisplayOperationId": true,
      "DefaultModelsExpandDepth": -1,
      "DefaultModelExpandDepth": 2,
      "DefaultModelRendering": "model",
      "DocExpansion": "full",
      "Filter": "",
      "ShowExtensions": true
    }
  }
}
